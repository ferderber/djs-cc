{
  "name": "djs-command-client",
  "version": "0.0.1",
  "description": "A Discord.js 'extension' that allows for easy creation of commands.",
  "main": "build/index.js",
  "typings": "build/index",
  "scripts": {
    "start": "npm run build && node build/index.js",
    "build": "tsc",
    "prepublish": "npm run build",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "tests(\\|/).+\\.spec\\.js$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/matthewferderber/djs-cc.git"
  },
  "keywords": [
    "Bot",
    "Discord"
  ],
  "author": "Matthew Ferderber",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matthewferderber/djs-cc/issues"
  },
  "homepage": "https://github.com/matthewferderber/djs-cc#readme",
  "dependencies": {
    "discord.js": "^11.1.0",
    "typeorm": "0.0.11",
    "reflect-metadata": "^0.1.10"
  },
  "optionalDependencies": {
    "sqlite3": "^3.1.8",
    "mysql": "^2.13.0"
  },
  "devDependencies": {
    "typescript": "^2.3.3",
    "@types/jest": "^19.2.3",
    "@types/node": "^7.0.21",
    "eslint": "^3.19.0",
    "jest": "^20.0.4",
    "ts-jest": "^20.0.4",
    "tslint": "^5.3.2"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 8
    },
    "env": {
      "node": true
    }
  }
}
