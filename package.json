{
  "name": "djs-cc",
  "version": "0.8.4",
  "description": "A Discord.js 'extension' that allows for easy creation of commands.",
  "main": "build/index.js",
  "typings": "build/index",
  "types": "build/index.d.ts",
  "scripts": {
    "start": "npm run build && node index.js",
    "start:dev": "nodemon -e ts -w ./src -x npm run start",
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "lint": "tslint -c tslint.json --project .",
    "docs": "typedoc --out ./docs --exclude index",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "cacheDirectory": "./node_modules/.cache/jest",
    "transform": {
      "\\.ts$": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/tests/.*(\\.|/)(test|spec))\\.jsx?$",
    "coveragePathIgnorePatterns": [
      "<rootDir>/tests",
      "<rootDir>/src/commands",
      "<rootDir>/node_modules"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "tsx",
      "json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/matthewferderber/djs-cc.git"
  },
  "keywords": [
    "Bot",
    "Discord"
  ],
  "author": "Matthew Ferderber",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matthewferderber/djs-cc/issues"
  },
  "homepage": "https://github.com/matthewferderber/djs-cc#readme",
  "dependencies": {
    "@types/ws": "^7.2.2",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.22"
  },
  "optionalDependencies": {
    "mysql": "^2.18.1",
    "sqlite3": "^4.1.1"
  },
  "devDependencies": {
    "@types/jest": "^25.1.1",
    "@types/node": "^13.7.0",
    "discord.js": "^12.0.1",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "ts-jest": "^25.1.0",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 8
    },
    "env": {
      "node": true
    }
  }
}
